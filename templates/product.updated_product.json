        "accordion_item_yiGiwt": {
          "type": "accordion_item",
          "settings": {
            "accordion_title": "¿Qué ocurre si se retrasa mi entrega?",
            "accordion_content": "<p> If your order is delayed beyond 6–8 business days, contact us and we’ll investigate and update you promptly.</p>",
            "tab_association": "2"
