      let seconds = 0;
  
      const intervalId = setInterval(() => {
        seconds--;
        if (seconds < 0) {
          seconds = 59;
          minutes--;
        }
        if (minutes < 0) {
          minutes = 59;
          hours--;
        }
  
        const formattedTime = `${hours.toString()}:${minutes.toString().padStart(2, "0")}`;
        timerElement.textContent = formattedTime;
  
        if (hours === 0 && minutes === 0) {
          clearInterval(intervalId);
        }
      }, 1000);
